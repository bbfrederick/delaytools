rapidtide \
	fmri.nii.gz \
	../dst/dgsr_mi_None \
	--passes 1 \
	--oversampfac 5 \
	--despecklepasses 0 \
	--searchrange -10 30 \
	--sigmalimit 25.0 \
	--nprocs -1 \
	--pickleft \
	--pickleftthresh 0.3 \
	--peakfittype None \
	--spatialfilt 2 \
	--saveintermediatemaps \
	--similaritymetric mutualinfo \
	--ampthresh 0.1


rapidtide \
	fmri.nii.gz \
	../dst/dgsr_fastquad \
	--passes 3 \
	--oversampfac 5 \
	--searchrange -10 30 \
	--sigmalimit 25.0 \
	--nprocs -1 \
	--pickleft \
	--pickleftthresh 0.3 \
	--peakfittype fastquad \
	--spatialfilt 2  \
        --saveintermediatemaps \
	--similaritymetric correlation \
	--ampthresh 0.3 

