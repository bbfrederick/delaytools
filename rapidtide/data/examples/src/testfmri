#!/bin/bash

rapidtide \
        sub-RAPIDTIDETEST.nii.gz \
        ../dst/sub-RAPIDTIDETEST_refinefail \
	--spatialfilt 2 \
        --ampthresh 0.99 \
        --nprocs -1 \
        --passes 3 

rapidtide \
        sub-RAPIDTIDETEST.nii.gz \
        ../dst/sub-RAPIDTIDETEST \
	--spatialfilt 2 \
        --nprocs -1 \
        --passes 3 

