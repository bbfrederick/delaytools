#!/bin/bash

rapidtide \
	sub-TEST.nii.gz \
	../dst/sub-TEST_standardnames \
	--spatialfilt 2 \
        --nprocs -1 \
        --legacyoutput \
        --passes 3 

rapidtide \
	sub-TEST.nii.gz \
	../dst/sub-TEST \
	--spatialfilt 2 \
        --nprocs -1 \
        --passes 3 \
        --nolimitoutput

