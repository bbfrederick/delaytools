#!/bin/bash

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar \
        --bipolar \
        --peakfittype fastquad \
        --nprocs 1 \
        --denoising 

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar_multiproc \
        --bipolar \
        --peakfittype fastquad \
        --nprocs -1 \
        --denoising 

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar_mp_none \
        --singleproc_getNullDist \
        --singleproc_calcsimilarity \
        --singleproc_peakeval \
        --singleproc_fitcorr \
        --singleproc_glm \
        --bipolar \
        --peakfittype fastquad \
        --nprocs -1 \
        --denoising 

diffmaps ../dst/dgsr_denoisingpass1_bipolar ../dst/dgsr_denoisingpass1_bipolar_mp_none ../dst/diff_mp_none

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar_mp_getNullDist \
        --singleproc_calcsimilarity \
        --singleproc_peakeval \
        --singleproc_fitcorr \
        --singleproc_glm \
        --bipolar \
        --peakfittype fastquad \
        --nprocs -1 \
        --denoising 

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar_mp_calcsimilarity \
        --singleproc_getNullDist \
        --singleproc_peakeval \
        --singleproc_fitcorr \
        --singleproc_glm \
        --bipolar \
        --peakfittype fastquad \
        --nprocs -1 \
        --denoising 

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar_mp_peakeval \
        --singleproc_getNullDist \
        --singleproc_calcsimilarity \
        --singleproc_fitcorr \
        --singleproc_glm \
        --bipolar \
        --peakfittype fastquad \
        --nprocs -1 \
        --denoising 


rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar_mp_fitcorr \
        --singleproc_getNullDist \
        --singleproc_calcsimilarity \
        --singleproc_peakeval \
        --singleproc_glm \
        --bipolar \
        --peakfittype fastquad \
        --nprocs -1 \
        --denoising 

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_denoisingpass1_bipolar_mp_glm \
        --singleproc_getNullDist \
        --singleproc_calcsimilarity \
        --singleproc_peakeval \
        --singleproc_fitcorr \
        --bipolar \
        --peakfittype fastquad \
        --nprocs -1 \
        --denoising 

diffmaps ../dst/dgsr_denoisingpass1_bipolar ../dst/dgsr_denoisingpass1_bipolar_multiproc ../dst/diff_multivssingle
diffmaps ../dst/dgsr_denoisingpass1_bipolar ../dst/dgsr_denoisingpass1_bipolar_mp_getNullDist ../dst/diff_mp_getNullDist
diffmaps ../dst/dgsr_denoisingpass1_bipolar ../dst/dgsr_denoisingpass1_bipolar_mp_calcsimilarity ../dst/diff_mp_calcsimilarity
diffmaps ../dst/dgsr_denoisingpass1_bipolar ../dst/dgsr_denoisingpass1_bipolar_mp_peakeval ../dst/diff_mp_peakeval
diffmaps ../dst/dgsr_denoisingpass1_bipolar ../dst/dgsr_denoisingpass1_bipolar_mp_fitcorr ../dst/diff_mp_fitcorr
diffmaps ../dst/dgsr_denoisingpass1_bipolar ../dst/dgsr_denoisingpass1_bipolar_mp_glm ../dst/diff_mp_glm

